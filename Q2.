Create or replace TRIGGER STUD1
BEFORE INSERT ON STUDENT
FOR EACH ROW
ENABLE
BEGIN
INSERT INTO TRACKING(STUDENTID,FIRST,LAST,STREET,CITY,STATE,ZIP,STARTTERM,BIRTHDATE,FACULTYID,MAJORID,PHONE) VALUES (:NEW.STUDENTID,:NEW.FIRST,:NEW.LAST,:NEW.STREET,:NEW.CITY,:NEW.STATE,:NEW.ZIP,:NEW.STARTTERM,:NEW.BIRTHDATE,:NEW.FACULTYID,:NEW.MAJORID,:NEW.PHONE);
END;
